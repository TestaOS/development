NAME='libtirpc'
VERSION='1.3.2'
DIRECTORY='libtirpc-1.3.2'
FILENAME='libtirpc-1.3.2.tar.bz2'
SOURCE="https://downloads.sourceforge.net/libtirpc/$FILENAME"

build() {

	./configure \
	--prefix=/usr \
	--sysconfdir=/etc \
	--enable-static \
	--disable-shared \
	--disable-gssapi

	make -j$(nproc) V=1
}
