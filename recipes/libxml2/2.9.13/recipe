NAME='libxml2'
VERSION='2.9.13'
DIRECTORY='libxml2-v2.9.13'
DEPENDENCIES='libtool-2.4.6'
FILENAME='libxml2-v2.9.13.tar.gz'
SOURCE="https://gitlab.gnome.org/GNOME/libxml2/-/archive/v2.9.13/$FILENAME"

build() {
	./autogen.sh \
	--prefix=/usr \
        --enable-static \
        --disable-shared

        make -j$(nproc) V=1
}
